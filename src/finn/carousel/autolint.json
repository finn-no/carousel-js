{
    "paths": [
        "*.js"
    ],
    "excludes": [
        "ckeditor",
        "jquery",
        "raphael",
        "underscore",
        "webtrends"
    ],
    "linterOptions": {
        "plusplus": false,
        "browser": true,
        "nomen": false,
        "maxlen": 120,
        "regexp": false,
        "onevar": false,
        "predef": [
            "jQuery", "FINN", "_"
        ]
    }
}
